{
    "configurations": [
        {
            "name": "Win32",
            "includePath": [
                "${workspaceFolder}/**",
                "${workspaceFolder}/MiniUI/Types/Inc/**",
                "${workspaceFolder}/MiniUI/Font/Inc/**",
                "${workspaceFolder}/MiniUI/Controls/Inc/**"
            ],
            "defines": [
                "_DEBUG",
                "UNICODE",
                "_UNICODE",
                "MINIUI_BGR888"
            ],
            "compilerPath": "C:/MinGW/bin/g++.exe",
            "intelliSenseMode": "windows-gcc-x86",
            "compilerArgs": [
                "-lbgi",
                "-lgdi32",
                "-lcomdlg32",
                "-luuid",
                "-loleaut32",
                "-lole32"
            ],
            "cStandard": "c11"
        }
    ],
    "version": 4
}